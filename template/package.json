    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config=build/prod.conf.js",
    "postbuild": "node build/link-dist.js",
    "dev": "cross-env NODE_ENV=development webpack-serve --config=./build/dev.conf.js --host {{ host }} --port {{ port }}",
