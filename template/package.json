    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config=build/prod.conf.js",
        "postbuild": "node build/link-dist.js",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --quiet --config=./build/dev.conf.js --host {{ host }} --port {{ port }}",
        "test-build": "rm -rf test-build/ && cross-env NODE_ENV=testing webpack --hide-modules --colors --progress --config=build/prod.conf.js",
        "dll": "webpack --config=build/dll.conf.js",
        "profile": "cross-env NODE_ENV=production webpack --profile --json --config=build/prod.conf.js > stat.json",
        "lint": "eslint --ext .js,.vue,.jsx --ignore-pattern [vendor] src",
        "lint-fix": "eslint --fix --ext .js,.vue,.jsx --ignore-pattern [vendor] src",
        "eslint": "eslint",
        "git-hook": "ln -s ../../hooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit"
    },
