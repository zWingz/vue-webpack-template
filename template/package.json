{
    "name": "{{ name }}",
    "version": "1.0.0",
    "description": "{{ description }}",
    "main": "index.js",
    "author": "{{ author }}",
    "license": "MIT",
    "scripts": {
        "clean-build": "rm -rf dist/",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config=build/prod.conf.js",
        "test-build": "rm -rf test-build/ && cross-env NODE_ENV=testing webpack --hide-modules --colors --progress --config=build/prod.conf.js",
        "postbuild": "rm -rf dist/* && cp -r release/* dist/ && rm -rf release",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --config=./build/dev.conf.js --hot --host {{ host }} --port {{ port }}",
        "dll": "webpack --config=build/dll.conf.js",
        "profile": "cross-env NODE_ENV=production webpack --profile --json --config=build/prod.conf.js > stat.json",
        "lint": "eslint --ext .js,.vue --ignore-pattern [vendor] src",
        "lint-fix": "eslint --fix --ext .js,.vue --ignore-pattern [vendor] src"
    },
    "devDependencies": {
        "babel-eslint": "^7.2.3",
        "eslint": "3.19.0",
        "eslint-config-airbnb-base": "^11.2.0",
        "eslint-plugin-html": "^3.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-plugin-vue-libs": "^1.2.1",
        "webpack-dev-server": "^2.6.1",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "webpack-hot-middleware": "^2.18.2"
    },
    "dependencies": {
        "autoprefixer": "^7.1.2",
        "axios": "^0.16.2",
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2016": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.1",
        "cssnano": "^3.10.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^2.30.1",
        "json-loader": "^0.5.7",
        "optimize-css-assets-webpack-plugin": "^3.1.1",
        "postcss-loader": "^2.0.6",
        "vue": "^2.4.2",
        "vue-loader": "^12.2.2",
        "vue-router": "^2.7.0",
        "vue-template-compiler": "^2.4.2",
        "vuex": "^2.4.0",
        "node-sass": "^4.5.3",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
        "webpack": "3.3.0",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dashboard": "^0.4.0",
        "webpack-merge": "^4.0.0"
    }
}
